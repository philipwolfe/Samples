<?xml version="1.0" encoding = "Windows-1252"?>
<VisualStudioProject
	ProjectType="Visual C++"
	Version="7.00"
	Name="Login">
	<Platforms>
		<Platform
			Name="Win32"/>
	</Platforms>
	<Configurations>
		<Configuration
			Name="Debug|Win32"
			OutputDirectory="Debug"
			IntermediateDirectory="Debug"
			ConfigurationType="2">
			<Tool
				Name="VCCLCompilerTool"
				Optimization="0"
				PreprocessorDefinitions="WIN32;_WINDOWS;_DEBUG;_AFXDLL;_MBCS;_USRDLL;_WINDLL;"
				MinimalRebuild="TRUE"
				ExceptionHandling="TRUE"
				RuntimeLibrary="3"
				BufferSecurityCheck="FALSE"
				UsePrecompiledHeader="3"
				WarningLevel="4"
				CompileOnly="TRUE"
				Detect64BitPortabilityProblems="TRUE"
				DebugInformationFormat="4"/>
			<Tool
				Name="VCCustomBuildTool"/>
			<Tool
				Name="VCLinkerTool"
				AdditionalDependencies="kernel32.lib user32.lib gdi32.lib winspool.lib comdlg32.lib advapi32.lib shell32.lib ole32.lib oleaut32.lib uuid.lib odbc32.lib odbccp32.lib"
				OutputFile="$(outdir)/Login.dll"
				LinkIncremental="2"
				GenerateDebugInformation="TRUE"
				SubSystem="2"
				ImportLibrary="$(outdir)/Login.lib"
				TargetMachine="1"/>
			<Tool
				Name="VCMIDLTool"
				PreprocessorDefinitions="_DEBUG"
				MkTypLibCompatible="FALSE"/>
			<Tool
				Name="VCPostBuildEventTool"/>
			<Tool
				Name="VCPreBuildEventTool"/>
			<Tool
				Name="VCPreLinkEventTool"/>
			<Tool
				Name="VCResourceCompilerTool"
				PreprocessorDefinitions="_DEBUG;_AFXDLL"/>
			<Tool
				Name="VCWebServiceProxyGeneratorTool"/>
			<Tool
				Name="VCWebDeploymentTool"
				ExcludedFromBuild="FALSE"
				RelativePath="cgi-bin"
				VirtualDirectoryName="MantaWeb"/>
		</Configuration>
		<Configuration
			Name="Release|Win32"
			OutputDirectory="Release"
			IntermediateDirectory="Release"
			ConfigurationType="2">
			<Tool
				Name="VCCLCompilerTool"
				Optimization="2"
				InlineFunctionExpansion="1"
				OmitFramePointers="TRUE"
				PreprocessorDefinitions="WIN32;_WINDOWS;NDEBUG;_AFXDLL;_MBCS;_USRDLL;_WINDLL;"
				StringPooling="TRUE"
				ExceptionHandling="TRUE"
				RuntimeLibrary="2"
				BufferSecurityCheck="FALSE"
				EnableFunctionLevelLinking="TRUE"
				UsePrecompiledHeader="3"
				WarningLevel="3"
				CompileOnly="TRUE"
				DebugInformationFormat="3"/>
			<Tool
				Name="VCCustomBuildTool"/>
			<Tool
				Name="VCLinkerTool"
				AdditionalDependencies="kernel32.lib user32.lib gdi32.lib winspool.lib comdlg32.lib advapi32.lib shell32.lib ole32.lib oleaut32.lib uuid.lib odbc32.lib odbccp32.lib"
				OutputFile="$(outdir)/Login.dll"
				LinkIncremental="1"
				GenerateDebugInformation="TRUE"
				SubSystem="2"
				ImportLibrary="$(outdir)/Login.lib"
				TargetMachine="1"/>
			<Tool
				Name="VCMIDLTool"
				PreprocessorDefinitions="NDEBUG"
				MkTypLibCompatible="FALSE"/>
			<Tool
				Name="VCPostBuildEventTool"/>
			<Tool
				Name="VCPreBuildEventTool"/>
			<Tool
				Name="VCPreLinkEventTool"/>
			<Tool
				Name="VCResourceCompilerTool"
				PreprocessorDefinitions="NDEBUG;_AFXDLL"/>
			<Tool
				Name="VCWebServiceProxyGeneratorTool"/>
			<Tool
				Name="VCWebDeploymentTool"/>
		</Configuration>
	</Configurations>
	<Files>
		<Filter
			Name="Source Files"
			Filter="cpp;c;cxx;rc;def;r;odl;idl;hpj;bat;asm">
			<File
				RelativePath="Login.cpp">
			</File>
			<File
				RelativePath="Login.rc">
			</File>
			<File
				RelativePath="StdAfx.cpp">
				<FileConfiguration
					Name="Debug|Win32">
					<Tool
						Name="VCCLCompilerTool"
						UsePrecompiledHeader="1"/>
				</FileConfiguration>
				<FileConfiguration
					Name="Release|Win32">
					<Tool
						Name="VCCLCompilerTool"
						UsePrecompiledHeader="1"/>
				</FileConfiguration>
			</File>
		</Filter>
		<Filter
			Name="Header Files"
			Filter="h;hpp;hxx;hm;inl;inc">
			<File
				RelativePath="AddUserDB.h">
			</File>
			<File
				RelativePath="AddUserHandler.h">
			</File>
			<File
				RelativePath="Crypt.h">
			</File>
			<File
				RelativePath="ForgotPassDB.h">
			</File>
			<File
				RelativePath="ForgotPassHandler.h">
			</File>
			<File
				RelativePath="LoginDB.h">
			</File>
			<File
				RelativePath="LoginHandler.h">
			</File>
			<File
				RelativePath="resource.h">
			</File>
			<File
				RelativePath="StdAfx.h">
			</File>
		</Filter>
		<Filter
			Name="Web Files"
			Filter="srf;css;htm">
			<File
				RelativePath="confirm.htm">
			</File>
			<File
				RelativePath="forgotpass.srf"
				DeploymentContent="TRUE">
			</File>
			<File
				RelativePath="login.css"
				DeploymentContent="TRUE">
			</File>
			<File
				RelativePath="login.srf"
				DeploymentContent="TRUE">
			</File>
			<File
				RelativePath="mail_redirect.htm">
			</File>
			<File
				RelativePath="newuser.srf"
				DeploymentContent="TRUE">
			</File>
			<File
				RelativePath="thankyou.htm">
			</File>
		</Filter>
	</Files>
	<Globals>
	</Globals>
</VisualStudioProject>
