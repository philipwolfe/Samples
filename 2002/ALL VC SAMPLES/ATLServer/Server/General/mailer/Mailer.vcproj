				Name="VCCLCompilerTool"
				Optimization="2"
				InlineFunctionExpansion="1"
				PreprocessorDefinitions="WIN32;NDEBUG;_WINDOWS;_MBCS;_USRDLL;MAILER_EXPORTS"
				StringPooling="TRUE"
				ExceptionHandling="TRUE"
				RuntimeLibrary="0"
