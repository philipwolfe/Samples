<?xml version="1.0" encoding = "Windows-1252"?>
<VisualStudioProject
	ProjectType="Visual C++"
	Version="7.00"
	Name="ATLMovie"
	SccProjectName=""
	SccLocalPath="">
	<Platforms>
		<Platform
			Name="Win32"/>
	</Platforms>
	<Configurations>
		<Configuration
			Name="Debug|Win32"
			OutputDirectory=".\Debug"
			IntermediateDirectory=".\Debug"
			ConfigurationType="2"
			UseOfMFC="1">
			<Tool
				Name="VCCLCompilerTool"
				Optimization="0"
				PreprocessorDefinitions="_DEBUG;WIN32;_WINDOWS;_USRDLL;_ATL_ATTRIBUTES"
				RuntimeLibrary="1"
				PrecompiledHeaderFile=".\Debug/ATLMovie.pch"
				AssemblerListingLocation=".\Debug/"
				ObjectFile=".\Debug/"
				ProgramDataBaseFileName=".\Debug/"
				WarningLevel="4"
				WarnAsError="TRUE"
				Detect64BitPortabilityProblems="TRUE"
				DebugInformationFormat="4"/>
			<Tool
				Name="VCCustomBuildTool"
				Description="Registering ActiveX Control..."
				CommandLine="regsvr32 /s /c &quot;$(TargetPath)&quot;
echo regsvr32 exec. time &gt; &quot;$(OutDir)\regsvr32.trg&quot;
"
				Outputs="$(OutDir)\regsvr32.trg"/>
			<Tool
				Name="VCLinkerTool"
				AdditionalDependencies="odbc32.lib odbccp32.lib"
				OutputFile=".\Debug/ATLMovie.dll"
				LinkIncremental="2"
				IgnoreDefaultLibraryNames="msvcrt.lib"
				GenerateDebugInformation="TRUE"
				ProgramDatabaseFile=".\Debug/ATLMovie.pdb"
				SubSystem="2"
				ImportLibrary=".\Debug/ATLMovie.lib"
				TargetMachine="1"/>
			<Tool
				Name="VCMIDLTool"
				PreprocessorDefinitions="_DEBUG"
				TargetEnvironment="1"
				TypeLibraryName="_ATLMovie.tlb"/>
			<Tool
				Name="VCPostBuildEventTool"/>
			<Tool
				Name="VCPreBuildEventTool"/>
			<Tool
				Name="VCPreLinkEventTool"/>
			<Tool
				Name="VCResourceCompilerTool"
				PreprocessorDefinitions="_DEBUG"/>
			<Tool
				Name="VCWebServiceProxyGeneratorTool"/>
		</Configuration>
		<Configuration
			Name="Release|Win32"
			OutputDirectory=".\ReleaseMinDependency"
			IntermediateDirectory=".\ReleaseMinDependency"
			ConfigurationType="2"
			UseOfMFC="1">
			<Tool
				Name="VCCLCompilerTool"
				Optimization="1"
				InlineFunctionExpansion="1"
				PreprocessorDefinitions="NDEBUG;_ATL_STATIC_REGISTRY;WIN32;_WINDOWS;_USRDLL;_ATL_ATTRIBUTES"
				StringPooling="TRUE"
				RuntimeLibrary="0"
				EnableFunctionLevelLinking="TRUE"
				PrecompiledHeaderFile=".\ReleaseMinDependency/ATLMovie.pch"
				AssemblerListingLocation=".\ReleaseMinDependency/"
				ObjectFile=".\ReleaseMinDependency/"
				ProgramDataBaseFileName=".\ReleaseMinDependency/"
				WarningLevel="4"
				WarnAsError="TRUE"
				Detect64BitPortabilityProblems="TRUE"/>
			<Tool
				Name="VCCustomBuildTool"
				Description="Registering ActiveX Control..."
				CommandLine="regsvr32 /s /c &quot;$(TargetPath)&quot;
echo regsvr32 exec. time &gt; &quot;$(OutDir)\regsvr32.trg&quot;
"
				Outputs="$(OutDir)\regsvr32.trg"/>
			<Tool
				Name="VCLinkerTool"
				AdditionalDependencies="odbc32.lib odbccp32.lib"
				OutputFile=".\ReleaseMinDependency/ATLMovie.dll"
				LinkIncremental="1"
				IgnoreDefaultLibraryNames="msvcrt.lib"
				SubSystem="2"
				ImportLibrary=".\ReleaseMinDependency/ATLMovie.lib"
				TargetMachine="1"/>
			<Tool
				Name="VCMIDLTool"
				PreprocessorDefinitions="NDEBUG"
				TargetEnvironment="1"
				TypeLibraryName="_ATLMovie.tlb"/>
			<Tool
				Name="VCPostBuildEventTool"/>
			<Tool
				Name="VCPreBuildEventTool"/>
			<Tool
				Name="VCPreLinkEventTool"/>
			<Tool
				Name="VCResourceCompilerTool"
				PreprocessorDefinitions="NDEBUG"/>
			<Tool
				Name="VCWebServiceProxyGeneratorTool"/>
		</Configuration>
	</Configurations>
	<Files>
		<Filter
			Name="Source Files"
			Filter="cpp;c;cxx;rc;def;r;odl;idl;hpj;bat">
			<File
				RelativePath=".\ATLMovie.cpp">
			</File>
			<File
				RelativePath=".\ATLMovie.rc">
			</File>
			<File
				RelativePath=".\MovieCtl.cpp">
			</File>
			<File
				RelativePath=".\StdAfx.cpp">
			</File>
		</Filter>
		<Filter
			Name="Header Files"
			Filter="h;hpp;hxx;hm;inl">
			<File
				RelativePath=".\MovieCtl.h">
			</File>
			<File
				RelativePath=".\Resource.h">
			</File>
			<File
				RelativePath=".\StdAfx.h">
			</File>
		</Filter>
		<Filter
			Name="Resource Files"
			Filter="ico;cur;bmp;dlg;rc2;rct;bin;cnt;rtf;gif;jpg;jpeg;jpe">
		</Filter>
		<File
			RelativePath=".\MovieCtl.rgs">
		</File>
	</Files>
	<Globals>
	</Globals>
</VisualStudioProject>
