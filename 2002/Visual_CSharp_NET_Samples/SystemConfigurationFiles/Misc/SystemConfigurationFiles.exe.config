<configuration>

	<configSections>
		<sectionGroup name="gridSettings">
			<section name="gridColumns" type="System.Configuration.NameValueSectionHandler" />
		</sectionGroup>
	</configSections>

	<gridSettings>
		<gridColumns>
			<add key="Column1" value="Setting Name" />
			<add key="Column2" value="Setting Value" />
		</gridColumns>
	</gridSettings>

	<appSettings>
    <add key="Host" value="http://myWebServices.com" />
    <add key="ServicePath" value="/SampleWS/" />
    <add key="Port" value="80" />
    <add key="Timeout" value="30000" />
    <add key="UseSSL" value="false" />
    <add key="WSDLFile" value="MySampleWS.wsdl" />
    <add key="WSMLFile" value="MySampleWS.wsml" />
    <add key="WebServiceName" value="SampleService" />
    <add key="WebServicePort" value="SampleServiceSOAPPort" />
	</appSettings>

</configuration>
