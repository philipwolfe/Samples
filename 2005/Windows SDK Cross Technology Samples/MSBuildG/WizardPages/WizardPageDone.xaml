<wpf:WizardPageFunction x:Class="Microsoft.Samples.MSBuildG.WizardPageDone"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:wpf="clr-namespace:Microsoft.Samples.MSBuildG"
  Style="{DynamicResource WizardPageStyle}" 
	Title="Creating Project..."
	Instructions="Please wait while your project is being created"
	Loaded="WindowLoaded"
    >
	<Grid Name="ContentGrid" Visibility="Collapsed">

		<StackPanel Orientation="Vertical">
			<Button Name="FileLink" Style="{DynamicResource WizardLinkButton}" Click="ProjectButtonClicked">ProjectFileLocation</Button>
			<Button Style="{DynamicResource WizardImageButton}" Width="150" Margin="4,4,4,4" Click="OpenProjectClicked">Open Project</Button>
			<Rectangle Height="50"/>
		</StackPanel>		
	</Grid>
</wpf:WizardPageFunction>
