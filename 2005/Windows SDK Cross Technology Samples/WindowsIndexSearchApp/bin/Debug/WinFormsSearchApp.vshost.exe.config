<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <configSections>
        <sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" >
            <section name="WinFormsSearchApp.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser"  />
        </sectionGroup>
    </configSections>
    <connectionStrings>
        <add name="WinFormsSearchApp.Properties.Settings.ConnectionString"
            connectionString="Provider=Search.CollatorDSO;Extended Properties=&quot;Application=Windows&quot;"
            providerName="System.Data.OleDb" />
    </connectionStrings>
    <userSettings>
        <WinFormsSearchApp.Properties.Settings>
            <setting name="Queries" serializeAs="Xml">
                <value>
                    <ArrayOfString xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                        xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                      <string>SELECT "System.FileName", "System.ItemType"
 FROM SYSTEMINDEX..SCOPE()
                      </string>
                      <string>SELECT "System.FileName", "System.Author" FROM SYSTEMINDEX..SCOPE()
 WHERE "Scope"='file:E:/Users/Public/Documents/Administrative'
 AND CONTAINS('Eva')
                      </string>
                      <string>SELECT "System.ItemUrl"
 FROM SYSTEMINDEX..SCOPE()
 WHERE "Scope"='file:E:/Users/Public/Documents/Administrative'
 AND CONTAINS('"Lorem ipsum"')
                      </string>
                      <string>SELECT "System.ItemUrl"
 FROM SYSTEMINDEX..SCOPE()
 WHERE "DIRECTORY"='file:E:/Program Files/Microsoft SDKs/Windows/v6.0/Include'
                      </string>
                      <string>SELECT "System.ItemUrl"
 FROM SYSTEMINDEX..SCOPE()
 WHERE "DIRECTORY"='file:E:/Program Files/Microsoft SDKs/Windows/v6.0/Include'
 AND CONTAINS('ShellExecuteEx')
                      </string>
                    </ArrayOfString>
                </value>
            </setting>
            <setting name="FontSize" serializeAs="String">
                <value>16</value>
            </setting>
        </WinFormsSearchApp.Properties.Settings>
    </userSettings>
</configuration>