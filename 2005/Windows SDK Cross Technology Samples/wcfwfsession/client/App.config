<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <system.serviceModel>
        <client>
            <endpoint address="http://localhost:8000/Calculator"
                binding="wsDualHttpBinding" bindingConfiguration="callback" contract="Microsoft.ServiceModel.Samples.ICalculator"/>

        </client>
      <bindings>
        <wsDualHttpBinding>
          <binding name="callback" clientBaseAddress="http://localhost:8001/CalculatorCallback/" />
        </wsDualHttpBinding>
      </bindings>
    </system.serviceModel>
</configuration>