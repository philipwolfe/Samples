<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:tns="uri:labserver" elementFormDefault="qualified" targetNamespace="uri:labserver" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="AskQuestionRequest">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" name="emailAddress" type="xs:string" />
        <xs:element minOccurs="0" name="question" type="xs:string" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="AskQuestionResponse">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="returnValue" type="xs:int" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>