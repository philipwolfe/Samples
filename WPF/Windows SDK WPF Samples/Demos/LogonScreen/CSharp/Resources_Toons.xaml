  <!-- Password TextBox Style -->
  <Style x:Key="PasswordTextBox"
         TargetType="{x:Type PasswordBox}">
    <Setter Property="Width"
            Value="350" />
    <Setter Property="Height"
            Value="50" />
    <Setter Property="Margin"
            Value="20" />
    <Setter Property="Grid.Row"
            Value="1" />
    <Setter Property="FontSize"
            Value="30" />
    <Setter Property="RenderTransform">
      <Setter.Value>
        <TransformGroup>
          <TransformGroup.Children>
            <TransformCollection>
              <TranslateTransform X="0"
                                  Y="-25" />
              <RotateTransform Angle="5" />
            </TransformCollection>
          </TransformGroup.Children>
        </TransformGroup>
      </Setter.Value>
    </Setter>
    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="{x:Type PasswordBox}">
          <Grid Width="350"
                Height="50">
            <Viewbox Stretch="Fill">
              <Path x:Name="ButtonBG"
                    Fill="white"
                    Stroke="#000000"
                    StrokeThickness="3"
                    Data="M150.655,39.109L10.407,53.785L0.602,1.309l158.026-0.806L150.655,39.109z" />
            </Viewbox>
            <ScrollViewer x:Name="PART_ContentHost"
                          Margin="40,0,20,0"
                          HorizontalScrollBarVisibility="Hidden"
                          VerticalScrollBarVisibility="Hidden"
                          Padding="{TemplateBinding Padding}" Focusable="false">
            </ScrollViewer>
          </Grid>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
    <Style.Triggers>
      <Trigger Property="IsEnabled"
               Value="False">
        <Setter Property="Background"
                Value="{DynamicResource {x:Static SystemColors.ControlBrushKey}}" />
        <Setter Property="Foreground"
                Value="{DynamicResource {x:Static SystemColors.GrayTextBrushKey}}" />
      </Trigger>
      <!-- Width -->
      <Trigger Property="Width"
               Value="Auto">
        <Setter Property="MinWidth"
                Value="100" />
      </Trigger>
    </Style.Triggers>
  </Style>
