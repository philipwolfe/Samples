<Project DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <!-- MSBUILD Project File -->
  <PropertyGroup>
    <AssemblyName>SDKSample</AssemblyName>
    <TargetType>winexe</TargetType>
    <Configuration>Release</Configuration>
    <ProductVersion>8.0.50727</ProductVersion>
    <SchemaVersion>2.0</SchemaVersion>
    <OutputPath>bin\$(Configuration)\</OutputPath>
  </PropertyGroup>
  <!--Import the target file that contains all the common targets -->
  <Import Project="$(MSBuildBinPath)\Microsoft.CSharp.targets" />
  <Import Project="$(MSBuildBinPath)\Microsoft.WinFX.targets" />
  <ItemGroup>
    <!-- Aplication Definition Markup File -->
    <ApplicationDefinition Include="app.xaml">
      <SubType>
      </SubType>
    </ApplicationDefinition>
    <!-- Compiled Xaml Files -->
    <Page Include="default.xaml" />
    <Page Include="document.xaml" />
    <Page Include="document1.xaml" />
    <Page Include="document2.xaml" />
  </ItemGroup>
  <ItemGroup>
    <Reference Include="System" />
    <Reference Include="System.Xml" />
    <Reference Include="System.Data" />
    <Reference Include="UIAutomationProvider" />
    <Reference Include="WindowsBase" />
    <Reference Include="PresentationCore" />
    <Reference Include="PresentationFramework" />
    <Reference Include="UIAutomationTypes" />
  </ItemGroup>
  <ItemGroup>
    <Compile Include="Default.xaml.cs">
      <DependentUpon>Default.xaml</DependentUpon>
      <SubType>Code</SubType>
    </Compile>
    <Compile Include="document.xaml.cs">
      <DependentUpon>document.xaml</DependentUpon>
      <SubType>Code</SubType>
    </Compile>
    <Compile Include="document1.xaml.cs">
      <DependentUpon>document1.xaml</DependentUpon>
      <SubType>Code</SubType>
    </Compile>
    <Compile Include="document2.xaml.cs">
      <DependentUpon>document2.xaml</DependentUpon>
      <SubType>Code</SubType>
    </Compile>
  </ItemGroup>
  <ItemGroup>
    <Resource Include="images\smiley.PNG" />
    <Resource Include="images\story1.PNG" />
    <Resource Include="images\story2.PNG" />
    <Resource Include="images\story3.PNG" />
    <Resource Include="images\tulip_farm.png" />
  </ItemGroup>
</Project>