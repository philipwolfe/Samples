            <RadioButton Click="cg5" Margin="5,0,5,0">5</RadioButton>
            <RadioButton Click="cg10" Margin="5,0,5,0">10</RadioButton>
            <RadioButton Click="cg15" Margin="5,0,5,0">15</RadioButton>
            <RadioButton Click="cg20" Margin="5,0,5,0">20</RadioButton>
        </StackPanel>
        </Grid>

        <!-- Use this control grouping to change ColumnRuleBrush Property Values -->
        <Grid Name="columnrulebrushProp" Visibility="Hidden">
