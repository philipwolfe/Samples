          <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
          </Grid.ColumnDefinitions>
          <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
          </Grid.RowDefinitions>

          <TextBlock Grid.ColumnSpan="2" Grid.Column="0" Grid.Row="0"  FontSize="14" FontWeight="Bold">ColumnRuleWidth Property</TextBlock>
        <TextBlock Grid.Column="0" Grid.Row="1" Margin="5,0,5,0">Adjust the ColumnRuleWidth Property:</TextBlock>
        <StackPanel Grid.Column="1" Grid.Row="1">
            <RadioButton Click="crw1" Margin="5,0,5,0">1</RadioButton>
            <RadioButton Click="crw3" Margin="5,0,5,0">3</RadioButton>
            <RadioButton Click="crw5" Margin="5,0,5,0">5</RadioButton>
            <RadioButton Click="crw10" Margin="5,0,5,0">10</RadioButton>
