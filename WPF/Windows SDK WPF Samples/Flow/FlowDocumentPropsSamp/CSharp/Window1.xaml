            <ColumnDefinition/>
          </Grid.ColumnDefinitions>
          <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
          </Grid.RowDefinitions>

          <TextBlock Grid.ColumnSpan="2" Grid.Column="0" Grid.Row="0"  FontSize="14" FontWeight="Bold">ColumnRuleBrush Property</TextBlock>
        <TextBlock Grid.Column="0" Grid.Row="1" Margin="5,0,5,0">Adjust the ColumnRuleBrush Color:</TextBlock>
        <StackPanel Grid.Column="1" Grid.Row="1">
            <RadioButton Click="setcolruleGray" Margin="5,0,5,0">Gray</RadioButton>
            <RadioButton Click="setcolruleBlack" Margin="5,0,5,0">Black</RadioButton>
            <RadioButton Click="setcolruleRed" Margin="5,0,5,0">Red</RadioButton>
            <RadioButton Click="setcolruleBlue" Margin="5,0,5,0">Blue</RadioButton>
