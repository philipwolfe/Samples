<Window  
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"    
    x:Class="SDKSample.Window1" Loaded="windowLoaded"
    Title="FlowDocument Properties Sample">

	<DockPanel VerticalAlignment="Top" MinWidth="800" MinHeight="600" HorizontalAlignment="Left" Margin="10" LastChildFill="True" Background="AliceBlue">
	
    <TextBlock FontSize="20" FontFamily="Palatino Linotype" FontWeight="Bold" DockPanel.Dock="Top">FlowDocument Properties Sample</TextBlock>
	<TextBlock TextWrapping="Wrap" FontFamily="Palatino Linotype" DockPanel.Dock="Top" Margin="10">Use the button controls to the left and right to adjust or read the various property values of the FlowDocument element below.</TextBlock>
        
    <StackPanel Orientation="Vertical" Margin="10,0,10,0" DockPanel.Dock="Left">  
        <Button Background="Blue" Foreground="White" FontFamily="Palatino Linotype" Margin="10,0,0,5" Click="makebgVisible">Set Background</Button>
        <Button Background="Blue" Foreground="White" FontFamily="Palatino Linotype" Margin="10,0,0,5" Click="makecgVisible">Set ColumnGap</Button>     
        <Button Background="Blue" Foreground="White" FontFamily="Palatino Linotype" Margin="10,0,0,5" Click="makecrbVisible">Set ColumnRuleBrush</Button>  
        <Button Background="Blue" Foreground="White" FontFamily="Palatino Linotype" Margin="10,0,0,5" Click="makecrwVisible">Set ColumnRuleWidth</Button>  
        <Button Background="Blue" Foreground="White" FontFamily="Palatino Linotype" Margin="10,0,0,5" Click="makecwVisible">Set ColumnWidth</Button>  
        <Button Background="Blue" Foreground="White" FontFamily="Palatino Linotype" Margin="10,0,0,5" Click="makeceVisible">Set ContentEnd</Button>
        <Button Background="Blue" Foreground="White" FontFamily="Palatino Linotype" Margin="10,0,0,5" Click="makecsVisible">Set ContentStart</Button>
        <Button Background="Blue" Foreground="White" FontFamily="Palatino Linotype" Margin="10,0,0,5" Click="makefdVisible">Set FlowDirection</Button>
        <Button Background="Blue" Foreground="White" FontFamily="Palatino Linotype" Margin="10,0,0,5" Click="makeffVisible">Set FontFamily</Button>
        <Button Background="Blue" Foreground="White" FontFamily="Palatino Linotype" Margin="10,0,0,5" Click="makefsVisible">Set FontSize</Button>
        <Button Background="Blue" Foreground="White" FontFamily="Palatino Linotype" Margin="10,0,0,5" Click="makefstVisible">Set FontStretch</Button>
        <Button Background="Blue" Foreground="White" FontFamily="Palatino Linotype" Margin="10,0,0,5" Click="makefstyleVisible">Set FontStyle</Button>
        <Button Background="Blue" Foreground="White" FontFamily="Palatino Linotype" Margin="10,0,0,5" Click="makefwVisible">Set FontWeight</Button>
        <Button Background="Blue" Foreground="White" FontFamily="Palatino Linotype" Margin="10,0,0,5" Click="makefgVisible">Set Foreground</Button>
        <Button Background="Blue" Foreground="White" FontFamily="Palatino Linotype" Margin="10,0,0,5" Click="makebpeVisible">Set IsBackgroundPaginationEnabled</Button>
        <Button Background="Blue" Foreground="White" FontFamily="Palatino Linotype" Margin="10,0,0,5" Click="makecwfVisible">Set IsColumnWidthFlexible</Button>
    </StackPanel> 
        
    <StackPanel Margin="10,0,10,0" Orientation="Vertical" DockPanel.Dock="Right">  
        <Button Background="Blue" Foreground="White" FontFamily="Palatino Linotype" Margin="10,0,0,5" Click="makepcfVisible">Get IsPageCountFinal</Button>
        <Button Background="Blue" Foreground="White" FontFamily="Palatino Linotype" Margin="10,0,0,5" Click="makelhVisible">Set Lineheight</Button>
        <Button Background="Blue" Foreground="White" FontFamily="Palatino Linotype" Margin="10,0,0,5" Click="makemaxphVisible">Set MaxPageHeight</Button>
        <Button Background="Blue" Foreground="White" FontFamily="Palatino Linotype" Margin="10,0,0,5" Click="makemaxpwVisible">Set MaxPageWidth</Button>
        <Button Background="Blue" Foreground="White" FontFamily="Palatino Linotype" Margin="10,0,0,5" Click="makeminphVisible">Set MinPageHeight</Button>
        <Button Background="Blue" Foreground="White" FontFamily="Palatino Linotype" Margin="10,0,0,5" Click="makeminpwVisible">Set MinPageWidth</Button>
        <Button Background="Blue" Foreground="White" FontFamily="Palatino Linotype" Margin="10,0,0,5" Click="makepcountVisible">Get PageCount</Button>
        <Button Background="Blue" Foreground="White" FontFamily="Palatino Linotype" Margin="10,0,0,5" Click="makephVisible">Set PageHeight</Button>
        <Button Background="Blue" Foreground="White" FontFamily="Palatino Linotype" Margin="10,0,0,5" Click="makepwVisible">Set PageWidth</Button>
        <Button Background="Blue" Foreground="White" FontFamily="Palatino Linotype" Margin="10,0,0,5" Click="makeppVisible">Set PagePadding</Button>
        <Button Background="Blue" Foreground="White" FontFamily="Palatino Linotype" Margin="10,0,0,5" Click="makepsVisible">Set PageSize</Button>
        <Button Background="Blue" Foreground="White" FontFamily="Palatino Linotype" Margin="10,0,0,5" Click="maketaVisible">Set TextAlignment</Button>
        <Button Background="Blue" Foreground="White" FontFamily="Palatino Linotype" Margin="10,0,0,5" Click="maketeVisible">Set TextEffects</Button>
        <!--
        <Button Background="Blue" Foreground="White" FontFamily="Palatino Linotype" Margin="10,0,0,5" Click="makettVisible">Set TextTrimming</Button>
        <Button Background="Blue" Foreground="White" FontFamily="Palatino Linotype" Margin="10,0,0,5" Click="maketwVisible">Set TextWrap</Button>
        -->
        <Button Background="Blue" Foreground="White" FontFamily="Palatino Linotype" Margin="10,0,0,5" Click="maketpVisible">Set Typography</Button>
    </StackPanel> 
  
    <Grid Margin="10" DockPanel.Dock="Bottom">
      <Grid.ColumnDefinitions>
        <ColumnDefinition/>
        <ColumnDefinition/>
      </Grid.ColumnDefinitions>
      <Grid.RowDefinitions>
        <RowDefinition/>
        <RowDefinition/>
      </Grid.RowDefinitions>

      <StackPanel Grid.Row="0" Grid.Column="0"  Name="sp1" MaxWidth="250" Orientation="Horizontal">
        <!-- Use this control grouping to change Background Property Values -->
        <Grid Name="backgroundProp" Visibility="Hidden">
          <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
          </Grid.ColumnDefinitions>
          <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
          </Grid.RowDefinitions>

        <TextBlock Grid.ColumnSpan="2" Grid.Column="0" Grid.Row="0"  FontSize="14" FontWeight="Bold">Background Property</TextBlock>
        <TextBlock Grid.Column="0" Grid.Row="1" Margin="5,0,5,0">Adjust the Background Color:</TextBlock>
        <StackPanel Grid.Column="1" Grid.Row="1">
            <RadioButton Click="setBackgroundRed" Margin="5,0,5,0">Red</RadioButton>
            <RadioButton Click="setBackgroundBlue" Margin="5,0,5,0">Blue</RadioButton>
            <RadioButton Click="setBackgroundGreen" Margin="5,0,5,0">Green</RadioButton>
            <RadioButton Click="setBackgroundPurple" Margin="5,0,5,0">Purple</RadioButton>
            <RadioButton Click="setBackgroundWhite" Margin="5,0,5,0">White</RadioButton>
        </StackPanel>
        </Grid>
        
        <!-- Use this control grouping to change the ColumnGap Property Values -->
        <Grid Name="columngapProp" Visibility="Hidden">
          <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
          </Grid.ColumnDefinitions>
          <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
          </Grid.RowDefinitions>

        <TextBlock Grid.ColumnSpan="2" Grid.Column="0" Grid.Row="0"  FontSize="14" FontWeight="Bold">ColumnGap Property</TextBlock>
        <TextBlock Grid.Column="0" Grid.Row="1" Margin="5,0,5,0">Adjust the ColumnGap Property:</TextBlock>
