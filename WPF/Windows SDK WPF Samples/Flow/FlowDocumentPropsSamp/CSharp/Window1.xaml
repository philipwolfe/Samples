          <TextBlock Grid.ColumnSpan="2" Grid.Column="0" Grid.Row="0"  FontSize="14" FontWeight="Bold">ContentEnd Property</TextBlock>
        <TextBlock Grid.Column="0" Grid.Row="1" Margin="5,0,5,0">Set the value of the ContentEnd Property:</TextBlock>
            <Button Grid.Column="0" Grid.Row="2"  Click="getCEValue">Add text at the ContentEnd position.</Button>
        </Grid>
        
        <!-- Use this control grouping to change ContentStart Property Values -->
        <Grid Name="contentstartProp" Visibility="Hidden">
          <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
          </Grid.ColumnDefinitions>
          <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
          </Grid.RowDefinitions>

          <TextBlock Grid.ColumnSpan="2" Grid.Column="0" Grid.Row="0"  FontSize="14" FontWeight="Bold">ContentStart Property</TextBlock>
