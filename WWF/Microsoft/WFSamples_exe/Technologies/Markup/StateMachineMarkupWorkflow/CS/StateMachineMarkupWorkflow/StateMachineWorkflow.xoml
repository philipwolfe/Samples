<StateMachineWorkflowActivity x:Class="Microsoft.Samples.Workflow.StateMachineMarkupWorkflow.StateMachineWorkflow" InitialStateName="StartState" x:Name="StateMachineWorkflow" DynamicUpdateCondition="{x:Null}" CompletedStateName="CompletedState" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/workflow">
	<StateActivity x:Name="StartState">
		<EventDrivenActivity x:Name="eventDriven1">
			<DelayActivity TimeoutDuration="00:00:05" x:Name="delay1" />
			<CodeActivity x:Name="code1" ExecuteCode="Code1Handler" />
			<SetStateActivity x:Name="setState1" TargetStateName="State1" />
		</EventDrivenActivity>
	</StateActivity>
	<StateActivity x:Name="State1">
		<EventDrivenActivity x:Name="eventDriven2">
			<DelayActivity TimeoutDuration="00:00:02" x:Name="delay2" />
