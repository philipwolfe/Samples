                            WHERE      (userActivities.ActivityTypeName = 'EnterResolution') AND (userActivities.Complete = 1)) AS workflowCompleteInfo ON 
                      userActivities.WorkflowGuid = workflowCompleteInfo.WorkflowGuid
GROUP BY userActivities.WorkflowGuid, userActivitiesWorkItems.WorkItemName, workflowStartInfo.DateAssigned, workflowStartInfo.AssignedUserName, 
