                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="False">
                    <CommandText>SELECT     userActivities.WorkflowGuid, userActivitiesWorkItems.WorkItemName AS Name, whoStarted.DateAssigned AS StartedOn, 
                      whoStarted.AssignedUserName AS StartedBy, waitingOnWho.AssignedUserName AS WaitingOn
FROM         UserActivities AS userActivities INNER JOIN
                      UserActivitiesWorkItems AS userActivitiesWorkItems ON userActivities.ActivityGuid = userActivitiesWorkItems.ActivityGuid INNER JOIN
                          (SELECT     userActivities.ActivityGuid, userActivities.WorkflowGuid, userActivityAssignments.DateAssigned, userActivityAssignments.AssignedUser, 
                                                   assignedUser.UserName AS AssignedUserName
                            FROM          aspnet_Users AS assignedUser RIGHT OUTER JOIN
                                                   UserActivityAssignments AS userActivityAssignments ON 
                                                   assignedUser.UserId = userActivityAssignments.AssignedUser RIGHT OUTER JOIN
                                                   UserActivities AS userActivities ON userActivityAssignments.ActivityGuid = userActivities.ActivityGuid
                            WHERE      (userActivities.Description = 'Work Item Approval Request')) AS whoStarted ON 
                      userActivities.WorkflowGuid = whoStarted.WorkflowGuid INNER JOIN
                          (SELECT     userActivities.ActivityGuid, userActivities.WorkflowGuid, userActivityAssignments.DateAssigned, userActivityAssignments.AssignedUser, 
                                                   assignedUser.UserName AS AssignedUserName
                            FROM          aspnet_Users AS assignedUser RIGHT OUTER JOIN
                                                   UserActivityAssignments AS userActivityAssignments ON 
                                                   assignedUser.UserId = userActivityAssignments.AssignedUser RIGHT OUTER JOIN
                                                   UserActivities AS userActivities ON userActivityAssignments.ActivityGuid = userActivities.ActivityGuid) AS waitingOnWho ON 
                      userActivities.WorkflowGuid = waitingOnWho.WorkflowGuid
WHERE     (whoStarted.DateAssigned IS NOT NULL) AND (whoStarted.AssignedUserName IS NOT NULL) AND (waitingOnWho.AssignedUserName IS NOT NULL) 
                      AND (userActivities.Complete = 0)
GROUP BY userActivities.WorkflowGuid, userActivitiesWorkItems.WorkItemName, whoStarted.DateAssigned, whoStarted.AssignedUserName, 
                      waitingOnWho.AssignedUserName
ORDER BY StartedOn</CommandText>
                    <Parameters>
                    </Parameters>
                  </DbCommand>
                </SelectCommand>
              </DbSource>
