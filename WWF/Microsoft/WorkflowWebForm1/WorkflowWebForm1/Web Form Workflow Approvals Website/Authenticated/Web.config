<?xml version="1.0"?>
<configuration>

	<system.web>
		<authorization>
			<deny users="?"/>
		</authorization>
	</system.web>

	<location path="Administration.aspx">
		<system.web>
			<authorization>
				<allow roles ="Administrator"/>
				<deny users="*"/>
			</authorization>
		</system.web>
	</location>

</configuration>
