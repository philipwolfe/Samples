<configuration>
  <system.web>
    <compilation targetFramework="4.0"/>
  </system.web>
  <system.serviceModel>
    <behaviors>
      <endpointBehaviors>
        <behavior>
          <synchronousReceive />
        </behavior>
      </endpointBehaviors>
    </behaviors>
  </system.serviceModel>
  <location path="Level3/Service5">
    <system.serviceModel>
      <behaviors>
        <serviceBehaviors>
          <behavior>
            <serviceThrottling />
          </behavior>
        </serviceBehaviors>
      </behaviors>
    </system.serviceModel>
  </location>
  <location path="Level3/Service7">
    <system.serviceModel>
      <behaviors>
        <endpointBehaviors>
          <behavior>
            <remove name="synchronousReceive" />
          </behavior>
        </endpointBehaviors>
      </behaviors>
    </system.serviceModel>
  </location>
</configuration>
