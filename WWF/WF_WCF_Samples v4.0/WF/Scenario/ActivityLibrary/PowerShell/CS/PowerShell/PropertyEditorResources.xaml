<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:PropertyEditing="clr-namespace:System.Activities.Presentation.PropertyEditing;assembly=System.Activities.Presentation" >

    <DataTemplate x:Uid="DataTemplate_9" x:Key="DynamicArgumentDictionaryInlineTemplate" >
        <Grid x:Uid="Grid_8" MinHeight="20">
            <Grid.ColumnDefinitions>
                <ColumnDefinition x:Uid="ColumnDefinition_9" Width="*"/>
                <ColumnDefinition x:Uid="ColumnDefinition_10" Width="Auto"/>
            </Grid.ColumnDefinitions>

            <TextBlock x:Uid="TextBlock_3" Grid.Column="0" Text="(Collection)"
                 VerticalAlignment="Center" HorizontalAlignment="Left"
                 TextTrimming="CharacterEllipsis"
                 />

            <PropertyEditing:EditModeSwitchButton x:Uid="PropertyEditing:EditModeSwitchButton_5" AutomationProperties.AutomationId="PropertyEditing:EditModeSwitchButton_1"
                 Grid.Column="1"
                 ToolTip="Edit items in this collection"/>

        </Grid>
    </DataTemplate>

</ResourceDictionary>
