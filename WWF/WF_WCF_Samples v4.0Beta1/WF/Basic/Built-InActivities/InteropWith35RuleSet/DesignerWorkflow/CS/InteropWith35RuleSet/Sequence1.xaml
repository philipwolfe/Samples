<p:Activity x:Class="Microsoft.Samples.TestInteropInDesigner.Sequence1" xmlns="clr-namespace:System.Activities.Design.Xaml;assembly=System.Activities.Design" xmlns:mst="clr-namespace:Microsoft.Samples.TravelRuleLibrary;assembly=TravelRuleLibrary" xmlns:p="http://schemas.microsoft.com/netfx/2009/xaml/activities" xmlns:sad="clr-namespace:System.Activities.Debugger;assembly=System.Activities" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
