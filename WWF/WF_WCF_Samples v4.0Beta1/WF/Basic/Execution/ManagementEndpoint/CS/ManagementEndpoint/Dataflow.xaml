<Sequence xmlns="http://schemas.microsoft.com/netfx/2009/xaml/activities" 
          xmlns:p="http://schemas.microsoft.com/netfx/2008/xaml/schema" 
          xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <Sequence.Variables>
    <Variable Name="str" x:TypeArguments="x:String" />
  </Sequence.Variables>
  <Assign x:TypeArguments="x:String" To="[str]" Value="hello, world" />
  <WriteLine Text="[str]" />
</Sequence>
